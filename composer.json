{
    "name": "onlinetest/onlinetest",
    "description": "online test",
    "authors": [
        {
            "name": "shamim shams",
            "email": "shamim.idb@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0",
        "eftec/bladeone": "^4.6",
        "codeigniter4/shield": "v1.0.0-beta.2",
        "illuminate/support": "^9.37",
        "eftec/bladeonehtml": "^2.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
